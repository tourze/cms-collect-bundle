{
  "name": "tourze/cms-collect-bundle",
  "description": "CMS内容收藏功能包，提供JSON-RPC接口和数据管理",
  "license": "MIT",
  "require": {
    "ext-random": "*",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "symfony/cache-contracts": "^3",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/event-dispatcher-contracts": "^3",
    "symfony/framework-bundle": "^7.4",
    "symfony/http-foundation": "^7.4",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/security-bundle": "^7.3",
    "symfony/security-core": "^7.3",
    "symfony/security-http": "^7.3",
    "symfony/serializer": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/cms-bundle": "0.0.*",
    "tourze/doctrine-indexed-bundle": "1.0.*",
    "tourze/doctrine-ip-bundle": "1.1.*",
    "tourze/doctrine-snowflake-bundle": "1.1.*",
    "tourze/doctrine-timestamp-bundle": "1.1.*",
    "tourze/doctrine-track-bundle": "1.1.*",
    "tourze/doctrine-user-bundle": "1.0.*",
    "tourze/easy-admin-attribute": "0.1.*",
    "tourze/easy-admin-menu-bundle": "1.0.*",
    "tourze/json-rpc-core": "2.0.*",
    "tourze/json-rpc-lock-bundle": "2.0.*",
    "tourze/json-rpc-log-bundle": "1.0.*",
    "tourze/symfony-dependency-service-loader": "1.0.*",
    "tourze/user-id-bundle": "1.0.*"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "nesbot/carbon": "^2.72 || ^3",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/phpunit-doctrine-entity": "1.*",
    "tourze/phpunit-json-rpc": "1.0.*",
    "tourze/phpunit-symfony-kernel-test": "2.0.*",
    "tourze/phpunit-symfony-unit-test": "2.0.*",
    "tourze/phpunit-symfony-web-test": "2.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\CmsCollectBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\CmsCollectBundle\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit"
  }
}
